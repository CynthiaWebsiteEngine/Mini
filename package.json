{
  "name": "@cynthiaweb/cynthiawebsiteengine-mini",
  "version": "1.0.0",
  "module": "cynthia_websites_mini_server/build/dev/javascript/cynthia_websites_mini_server/cynthia_websites_mini_server.ts",
  "bin": {
    "cynthiaweb-mini": "./dist/cynthia_websites_mini_server.js"
  },
  "type": "module",
  "devDependencies": {
    "@types/bun": "latest",
    "@types/sql.js": "^1.4.9",
    "nodemon": "^3.1.9"
  },
  "scripts": {
    "preinstall": "npx only-allow bun",
    "bundle": "bun ./build.ts bundle",
    "check": "bun ./build.ts check",
    "clean": "bun ./build.ts clean",
    "test": "bun ./build.ts test",
    "format": "bun ./build.ts fmt",
    "dev": "bun nodemon --ext gleam,ts --ignore ./cynthia_websites_mini_client/build/ --ignore ./cynthia_websites_mini_server/build/ --ignore prelude.ts --ignore cynthia_websites_mini_server/src/client_code_generated_ffi.ts  --exec bun ./build.ts run",
    "prepack": "bun run bundle",
    "run-in-test": "bun run bundle && cd ./test && bun ../dist/cynthia_websites_mini_server.js"
  },
  "dependencies": {
    "autoprefixer": "^10.4.20",
    "daisyui": "^4.12.23",
    "moment": "^2.30.1",
    "postcss": "^8.5.1",
    "smol-toml": "^1.3.1",
    "sql.js": "^1.12.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.0.0"
  }
}
